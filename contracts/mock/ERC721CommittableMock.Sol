pragma solidity ^0.8.0;

import "../ERC721/ERC721Committable.sol";

contract ERC721CommittableMock is ERC721Committable {
    /**
     * @dev Mint a token to address without signature check for easy test
     */
    function mint(address creator, uint256 tokenId) public virtual override {
        // _verify(creator, tokenId, signature);
        _mint(creator, tokenId);
        // claim reward if 1) msg.sender is token owner 2) have funds
        if (msg.sender == creator && fundsOf(tokenId) > 0) {
            claim(tokenId);
        }
    }
}
